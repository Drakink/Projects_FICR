#include <stdio.h>
#include <conio.h>

int main()
{
    int numero , contador , resultado;
    printf ("Sou uma tabuada Super automatica.ðŸ˜ˆ \n Digite Seu NÃºmero Aqui:");
    scanf ("%d",&numero);
    for (contador=1; contador<=10; contador++) {
        resultado = numero * contador;
        printf("%d * %d = %d\n", numero, contador, resultado);
    }
    return 0;
}
